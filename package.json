    "prettier": "prettier --write \"apps/**/*.{vue,js,ts}\"",
    "lint": "eslint . --fix",
    "postinstall": "node ./scripts/patch-kugoumusicapi.js"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.4.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^9.39.2",
    "eslint-plugin-format": "^1.4.0",
    "nexe": "5.0.0-beta.4",
    "prettier": "^3.8.1",
    "semantic-release": "^25.0.3",
    "typescript": "^5.9.3"
  }
}
