{
  "name": "kugou-exporter",
  "version": "1.1.1",
  "private": true,
  "packageManager": "pnpm@10.29.3",
  "description": "酷狗音乐歌单导出工具",
  "bin": {
    "kugou-exporter": "./dist/index.js"
  },
  "scripts": {
    "dev": "pnpm -r dev",
    "build": "pnpm -r build",
    "build:nexe": "pnpm build && node scripts/build.js",
